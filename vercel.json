{
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
  "build": {
    "env": {
      "SECRET_KEY": "@SECRET_KEY"
    }
  },
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}